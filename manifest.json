{
  "name": "YoutubeAmp",
  "description": "Creates a player for Youtube songs",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://secure.flickr.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
